{
    "name": "Presentator",
    "description": "Web design presentation and collaboration platform.",
    "keywords": ["yii2", "design", "presentation", "collaboration"],
    "homepage": "https://www.presentator.io/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/ganigeorgiev/presentator/issues?state=open",
        "source": "https://github.com/ganigeorgiev/presentator"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.0",
        "yiisoft/yii2": "2.0.13.3",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "yiisoft/yii2-authclient": "~2.1",
        "yiisoft/yii2-imagine": "~2.1",
        "yii2tech/ar-position": "~1.0",
        "dpodium/yii2-geoip": "~1.3.8",
        "himiklab/yii2-recaptcha-widget" : "~1.1.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "~2.4.0",
        "codeception/specify": "~0.4.0",
        "codeception/verify": "~0.4.0"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
